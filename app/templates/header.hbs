<!--suppress HtmlUnknownTarget -->
<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <div class="container-fluid">
		<div class="navbar-header">
			<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
				<span class="sr-only">Toggle navigation</span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
            {{#if linkTo}}
                {{#link-to linkTo classNames="navbar-brand"}}
                    <img alt="GoDeep Mobile" src="assets/img/logo.png">
                {{/link-to}}
            {{else}}
                {{#link-to 'dashboard' classNames="navbar-brand"}}
                    <img alt="GoDeep Mobile" src="assets/img/logo.png">
                {{/link-to}}
            {{/if}}
            <!--a class="navbar-brand" href="#">
                <img alt="GoDeep Mobile" src="assets/img/logo.png">
            </a-->
		</div>
        <div class="navbar-textalt navbar-collapse collapse">
            <span id="header_message_1center">{{message}}</span>
        </div><!--/.navbar-collapse -->
        <div class="navbar-textalt xnavbar-collapse xcollapse pull-right">
            <span id="header_pagename"class="block-color-featuretext">{{pageName}}</span>
        </div><!--/.navbar-collapse -->
    </div>
    <div class="container-fluid xblock-grid-row block-color-header">
        <!--div class="xblock block-grid-row-first"-->
        <span id="header_message_2left">{{message_2left}}</span>
        <span id="header_message_2right" class="pull-right">{{message_2right}}</span>
        <!--/div-->
    </div>
</div>
